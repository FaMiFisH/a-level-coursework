<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a_level_coursework">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AlevelCoursework">

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".MenuActivity"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".TimetableActivity"/>
        <activity android:name=".ClassesActivity"/>
        <activity android:name=".NotesActivity"/>
        <activity android:name=".ClassActivity"/>
        <activity android:name=".StudentActivity"/>
        <activity android:name=".ChooseClassActivity"/>
        <activity android:name=".RegActivity"/>

    </application>

</manifest>